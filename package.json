{
  "name": "tez-native",
  "version": "2.0.0-rc2",
  "description": "Mobile apps with native UIs in JavaScript",
  "main": "tabris.js",
  "keywords": [
    "native",
    "mobile",
    "ios",
    "android"
  ],
  "license": "BSD-3-Clause",
  "homepage": "http://tabrisjs.com/",
  "repository": {
    "type": "git",
    "url": "https://github.com/dalisoft/tez-native.git"
  },
  "devDependencies": {
    "rollup": "^0.41.6",
	"rollup-plugin-buble": "^0.15.0",
	"rollup-watch": "^3.2.2",
	"css-to-js-object": "latest",
    "es6-tween": "latest"
  },
  "scripts": {
    "build": "rollup -c --environment BUILD:prod",
