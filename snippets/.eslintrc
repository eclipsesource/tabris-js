{
  "parserOptions": {
    "ecmaVersion": 5
  },
  "globals": {
    "tabris": false,
    "window": false,
    "document": false,
    "navigator": false,
    "screen": false,
    "device": false,
    "fetch": false,
    "localStorage": false,
    "crypto": false,
    "cordova": false,
    "ArrayBuffer": false,
    "ImageData": false,
    "Int8Array": false,
    "Uint8Array": false,
    "Uint8ClampedArray": false,
    "Int16Array": false,
    "Uint16Array": false,
    "Uint32Array": false,
    "Int32Array": false,
    "Float32Array": false,
    "Float64Array": false,
    "Promise": false,
    "WebSocket": false
  },
  "rules": {
    "no-var": "off",
    "prefer-arrow-callback": "off"
  }
}
